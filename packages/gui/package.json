{
  "name": "scenario-editor",
  "private": "true",
  "version": "0.0.1",
  "description": "Scenario editor.",
  "scripts": {
    "clean": "rimraf ./public ./.cache ./dist",
    "cleanup": "npm run clean",
    "dev": "npm start",
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --out-dir ../server/public",
    "build:domain": "npm run clean && npm run build",
    "link": "pnpm link ass-shared"
  },
  "app": "./index.html",
  "targets": {
    "app": {
      "publicUrl": "/"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TNOCS/agent-smith.git"
  },
  "keywords": [
    "scenario",
    "editor"
  ],
  "author": "Erik Vullings <erik.vullings@tno.nl> (http://www.tno.nl)",
  "license": "MIT",
  "dependencies": {
    "material-icons": "^0.5.3",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.4",
    "mithril-materialized": "^0.17.1",
    "slimdown-js": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.12",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-function-sent": "7.12.1",
    "@babel/plugin-proposal-json-strings": "7.12.1",
    "@babel/plugin-proposal-numeric-separator": "7.12.7",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-proposal-throw-expressions": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-flow": "7.12.1",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/lokijs": "^1.5.3",
    "@types/proj4leaflet": "^1.0.5",
    "autoprefixer": "9.8.6",
    "babel-types": "^6.26.0",
    "@types/leaflet": "^1.5.23",
    "@types/materialize-css": "^1.0.8",
    "@types/mithril": "^2.0.6",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "typescript": "^4.2.2"
  }
}
