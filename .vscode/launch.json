  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Simulation",
      "cwd": "${workspaceFolder}/packages/ass/",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/packages/ass/node_modules/**/*.js"
      ],
      "program": "${workspaceFolder}\\packages\\ass\\src\\run.ts",
      "outFiles": [
        "${workspaceFolder}/packages/ass/dist/**/*.js"
      ],
    }
  ]
}